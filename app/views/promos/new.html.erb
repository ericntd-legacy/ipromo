<% provide(:title, 'Them khuyen mai') %>
<h1>Them khuyen mai</h1>
<div class="row">
  <div class="span6 offset3">
					<%= form_for(@promo) do |f| %>
									<%= render 'shared/error_messages' %>

      <%= f.label :name %>
      <%= f.text_field :name %>

      <%= f.label :desc %>
			<%= f.text_field :desc %>

			<%= f.label :imgsrc %>
      <%= f.text_field :imgsrc %>

	
      <%= f.submit "Luu lai", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>
